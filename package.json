{
	"dependencies": {
		"@prisma/client": "^6.14.0",
		"discord.js": "^14.21.0",
		"emittery": "^1.2.0",
		"lru-cache": "^11.1.0",
		"pino": "^9.9.0",
		"zod": "^4.0.17"
	},
	"devDependencies": {
		"@biomejs/biome": "^2.2.0",
		"@commitlint/config-conventional": "^19.8.1",
		"@feature-sliced/steiger-plugin": "^0.5.7",
		"@types/bun": "1.2.20",
		"@vitest/coverage-v8": "^3.2.4",
		"lefthook": "^1.12.3",
		"pino-pretty": "^13.1.1",
		"prisma": "^6.14.0",
		"steiger": "^0.5.11",
		"typescript": "^5.9.2",
		"vite": "^7.1.2",
		"vite-tsconfig-paths": "^5.1.4",
		"vitest": "^3.2.4",
		"vitest-mock-extended": "^3.1.0"
	},
	"module": "src/app/index.ts",
	"name": "necromancer",
	"scripts": {
		"build": "bun build src/app/index.ts --outdir ./dist --target bun",
		"check": "biome check --write",
		"check:ci": "biome ci .",
		"check:fsd": "steiger ./src",
		"check:tsc": "tsc --noEmit --skipLibCheck",
		"commands:deploy": "bun scripts/deploy-commands.ts",
		"prisma:generate": "prisma generate",
		"start": "bun --watch src/app/index.ts",
		"start:prod": "bunx prisma migrate deploy && bun dist/index.js",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage",
		"test:watch": "vitest watch",
		"version:increment": "bun scripts/increment-version.ts"
	},
	"type": "module",
	"version": "0.15.0"
}
